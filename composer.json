{
  "name": "symonwhite/phpunittraits",
  "description": "this repo is an summary of function for phpunit test",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^5.6 || ^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "psr-4": {
      "SymonWhite\\PhpUnitTraits\\": "src/",
      "SymonWhite\\PhpUnitTraits\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always",
    "test-coverage": "vendor/bin/phpunit --colors=always --coverage-html coverage"
  }
}
